#else
	#set($columnsToSelect=["HIER3_ID", "EVENT_ID", "HIER321_ID", "IMSI", "RELEASE_REQ_QCI", "EVENT_TIME", "TAC", "SETUP_REQ_ARP", "SETUP_REQ_PCI", "SETUP_REQ_PVI", "DATA_LOST", "RELEASE_SUCC"])
	#set($resultColumns=["err.HIER3_ID", "err.HIER321_ID", "EVENT_TIME", "IMSI", "err.TAC", "VENDOR_NAME", "MARKETING_NAME", "EVENT_ID_DESC", "topo.HIERARCHY_3", "topo.HIERARCHY_1", "topo.VENDOR", "'LTE' as RAT", "RELEASE_REQ_QCI", "SETUP_REQ_ARP", "SETUP_REQ_PCI_DESC", "SETUP_REQ_PVI_DESC", "dc.DIM_E_LTE_CFA_ERAB_DATA_LOST.DATA_LOST_DESC", "dc.DIM_E_LTE_CFA_ERAB_RELEASE_SUCC.RELEASE_SUCC_DESC"])
#end

select
	#if($count > 0)
	  top $count
	#end
    #GET_COLUMNS_NO_COMMA_TRAILING($resultColumns)
from
	#REPLACE_RAW_VIEW_WITH_RAW_TABLES_AND_FILTER_COLUMNS_WITH_TAC_EXCLUSION_CONDITION_COLUMNS_GROUPS($TECH_PACK_LIST.getAllRawTablesWithMeasurementType() "err" $condictionColumns $columnsToSelect),
	dc.DIM_E_LTE_CFA_EVENTTYPE,
    dc.DIM_E_LTE_CFA_ERAB_SETUP_REQ_PCI,
    dc.DIM_E_LTE_CFA_ERAB_SETUP_REQ_PVI,
    (	
		select
			topo.HIERARCHY_1,
			topo.HIERARCHY_3,
			topo.HIER3_ID,
			topo.HIER321_ID,
			topo.VENDOR
		from 
			dc.DIM_E_LTE_HIER321 topo,
			$groupTable groupTable
		where
			groupTable.GROUP_NAME = :$ApplicationConstants.GROUP_NAME_PARAM and
			groupTable.HIER3_ID = topo.HIER3_ID
		group by
			topo.HIER321_ID,
			topo.HIER3_ID,
			topo.HIERARCHY_1,
			topo.HIERARCHY_3,
			topo.VENDOR
	) as topo,
    dc.DIM_E_SGEH_TAC as dim_tac,
	#if($isSetupEvent)
    dc.DIM_E_LTE_CFA_ERAB_SETUP_RESULT,
    dc.DIM_E_LTE_CFA_ERAB_SETUP_ATT_ACC_TYPE,
    dc.DIM_E_LTE_CFA_ERAB_SETUP_SUCC_ACC_TYPE,
    dc.DIM_E_LTE_CFA_ERAB_SETUP_FAIL_3GPP_CAUSE_GROUP
	#else
	dc.DIM_E_LTE_CFA_ERAB_DATA_LOST,
	dc.DIM_E_LTE_CFA_ERAB_RELEASE_SUCC
	#end
where
	dc.DIM_E_LTE_CFA_EVENTTYPE.EVENT_ID = :eventID
    and err.EVENT_ID *= dc.DIM_E_LTE_CFA_EVENTTYPE.EVENT_ID
    and err.SETUP_REQ_PCI *= dc.DIM_E_LTE_CFA_ERAB_SETUP_REQ_PCI.SETUP_REQ_PCI
    and err.SETUP_REQ_PVI *= dc.DIM_E_LTE_CFA_ERAB_SETUP_REQ_PVI.SETUP_REQ_PVI
	and err.HIER321_ID *= topo.HIER321_ID
    and err.TAC *= dim_tac.TAC
	#if($isSetupEvent)
    and err.SETUP_RESULT *= dc.DIM_E_LTE_CFA_ERAB_SETUP_RESULT.SETUP_RESULT
    and err.SETUP_ATT_ACC_TYPE *= dc.DIM_E_LTE_CFA_ERAB_SETUP_ATT_ACC_TYPE.SETUP_ATT_ACC_TYPE
    and err.SETUP_SUCC_ACC_TYPE *= dc.DIM_E_LTE_CFA_ERAB_SETUP_SUCC_ACC_TYPE.SETUP_SUCC_ACC_TYPE
    and err.SETUP_FAIL_3GPP_CAUSE_GROUP *= dc.DIM_E_LTE_CFA_ERAB_SETUP_FAIL_3GPP_CAUSE_GROUP.SETUP_FAIL_3GPP_CAUSE_GROUP
	#else
	and err.RELEASE_SUCC *= dc.DIM_E_LTE_CFA_ERAB_RELEASE_SUCC.RELEASE_SUCC
	and err.DATA_LOST *= dc.DIM_E_LTE_CFA_ERAB_DATA_LOST.DATA_LOST
    #end