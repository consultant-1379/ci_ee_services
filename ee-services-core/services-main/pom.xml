					<version>${project.version}</version>
					
				</dependency>
			</dependencies>
			<build>
				<plugins>
					<plugin>
						<groupId>org.codehaus.mojo</groupId>
						<artifactId>aspectj-maven-plugin</artifactId>
						<configuration>
							<source>1.6</source>
							<target>1.6</target>
							<aspectLibraries>
								<aspectLibrary>
									<groupId>${project.groupId}</groupId>
									<artifactId>services-groovy</artifactId>
								</aspectLibrary>
							</aspectLibraries>
							<weaveDependencies>
								<weaveDependency>
									<groupId>${project.groupId}</groupId>
									<artifactId>services-templates</artifactId>
								</weaveDependency>
							</weaveDependencies>
						</configuration>
						<executions>
							<execution>
								<goals>
									<goal>compile</goal>
								</goals>
							</execution>
						</executions>
					</plugin>
				</plugins>
			</build>
		</profile>
