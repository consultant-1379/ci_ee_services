	<build>
		<finalName>EniqEventsServices</finalName>
		<plugins>


			<plugin>
				<artifactId>maven-resources-plugin</artifactId>
				<executions>


					<execution>
						<id>copy-war-for-release</id>
						<phase>package</phase>
						<goals>
							<goal>copy-resources</goal>
						</goals>
						<configuration>
							<outputDirectory>${basedir}/../install</outputDirectory>
							<resources>
								<resource>
									<directory>${project.build.directory}</directory>
									<includes>
										<include>${project.build.finalName}.war</include>
									</includes>
									<filtering>false</filtering>
								</resource>
							</resources>
						</configuration>
					</execution>
				</executions>
			</plugin>

			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-war-plugin</artifactId>
				<configuration>
					<!-- Hack here for known war plugin issue of including web.xml twice 
						- exclude it at this stage from being included in the package, when version 
						2.2 is released this should probably be removed -->
					<packagingExcludes>**/web.xml</packagingExcludes>
				</configuration>
			</plugin>

			<plugin>
				<groupId>com.ericsson.maven.plugins</groupId>
				<artifactId>MetaDataCompiler</artifactId>
				<version>1.2</version>

				<executions>
					<execution>
						<phase>generate-sources</phase>
						<goals>
							<goal>compile-meta-data</goal>
						</goals>
					</execution>
				</executions>

				<configuration>
					<baseDirectory>
						<baseDir>${basedir}/src/main/resources/uimetadata</baseDir>
					</baseDirectory>

					<outputFile>
						<param>${basedir}/src/main/resources/UIMetaData.json</param>
					</outputFile>
					<fileInclusionExtension>.json</fileInclusionExtension>
				</configuration>
			</plugin>

		</plugins>

		<testResources>
			<testResource>
				<directory>${basedir}/src/test/java</directory>
				<includes>
					<include>**/*.xml</include>
					<include>**/*.properties</include>
				</includes>
			</testResource>
			<testResource>
				<directory>${basedir}/src/test/resources</directory>
			</testResource>
		</testResources>

		<resources>
			<resource>
				<directory>${basedir}/src/main/resources</directory>
				<excludes> <!-- exclude any clearcase specific files, primarily for local builds -->
					<exclude>**/*.contrib</exclude>
					<exclude>**/*.keep</exclude>
					<exclude>**/*.mkelem</exclude>
					<exclude>**/*.checkin</exclude>
					<exclude>**/*.loading</exclude>
					<exclude>**/*.merge</exclude>
					<exclude>**/*.unloaded</exclude>
					<exclude>**/*.hijacked</exclude>
					<exclude>**/*.checkedout</exclude>
					<exclude>q_named_parameter_test.vm</exclude>
					<exclude>q_test.vm</exclude>
				</excludes>
			</resource>
		</resources>
		<pluginManagement>
			<plugins>
				<!--This plugin's configuration is used to store Eclipse m2e settings 
					only. It has no influence on the Maven build itself. -->
				<plugin>
					<groupId>org.eclipse.m2e</groupId>
					<artifactId>lifecycle-mapping</artifactId>
					<version>1.0.0</version>
					<configuration>
						<lifecycleMappingMetadata>
							<pluginExecutions>
								<pluginExecution>
									<pluginExecutionFilter>
										<groupId>
											com.ericsson.maven.plugins
										</groupId>
										<artifactId>
											MetaDataCompiler
										</artifactId>
										<versionRange>
											[1.2,)
										</versionRange>
										<goals>
											<goal>
												compile-meta-data
											</goal>
										</goals>
									</pluginExecutionFilter>
									<action>
										<ignore />
									</action>
								</pluginExecution>
							</pluginExecutions>
						</lifecycleMappingMetadata>
					</configuration>
				</plugin>
			</plugins>
		</pluginManagement>
	</build>

	<profiles>
		<profile>
			<id>groovy</id>
			<activation>
				<activeByDefault>false</activeByDefault>
			</activation>
			<properties>
				<groovyTarget>compile</groovyTarget>
				<coberturaTarget>instrument</coberturaTarget>
				<aspectj.groovy>process-classes</aspectj.groovy>
				<ant.groovy>process-test-classes</ant.groovy>
				<site.groovy>verify</site.groovy>
			</properties>
			<dependencies>
				<dependency>
					<groupId>${project.groupId}</groupId>
